<div class="container">
  <div class="row map-row">
    <div class="col-xs-12">
      <div id="map" style="width: 100%; height: 35vh;" data-markers="<%= @markers.to_json %>">
      </div>
    </div>
    <h6>Your Nearby Tennis Courts</h6>
  </div>
  <div class="users_list">
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-xs-4 user_each">
          <div class="thumbnail user-photo" style="width: 90px; height: 90px;" >
             <%= link_to user_path(user[:id]) do %>
            <%= image_tag user.photo %>
            <% end %>
          </div>
          <h6><%= user.first_name %></h6>
          <p>Skill level: <strong><%= user.skill_level %></strong></p>
        </div>
      <% end %>
      <div class="col-xs-4 user_each">
        <div style="width: 90px; height: 90px;"></div>
      </div>
    </div>
  </div>
</div>

